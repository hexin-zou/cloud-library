{"ast":null,"code":"import \"core-js/modules/es.error.cause.js\";\nimport request from \"@/utils/request\";\nexport default {\n  name: 'AddAdmin',\n\n  data() {\n    const checkPhone = (rule, value, callback) => {\n      if (!/^[1][3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\n        callback(new Error('请输入合法的手机号'));\n      }\n\n      callback();\n    };\n\n    return {\n      form: {},\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 10,\n          message: '长度在3-10个字符',\n          trigger: 'blur'\n        }],\n        phone: [{\n          validator: checkPhone,\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  methods: {\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          request.post('/admin/save', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success('新增成功');\n              this.$refs['ruleForm'].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAsBA;AAEA;EACAA,gBADA;;EAEAC;IACA;MACA;QACAC;MACA;;MACAA;IACA,CALA;;IAMA;MACAC,QADA;MAEAC;QACAC,WACA;UAAAC;UAAAC;UAAAC;QAAA,CADA,EAEA;UAAAC;UAAAC;UAAAH;UAAAC;QAAA,CAFA,CADA;QAKAG,QACA;UAAAC;UAAAJ;QAAA,CADA;MALA;IAFA;EAYA,CArBA;;EAsBAK;IACAC;MACA;QACA;UACAC;YACA;cACA;cACA;YACA,CAHA,MAGA;cACA;YACA;UACA,CAPA;QAQA;MACA,CAXA;IAYA;;EAdA;AAtBA","names":["name","data","callback","form","rules","username","required","message","trigger","min","max","phone","validator","methods","save","request"],"sourceRoot":"src/views/admin","sources":["Add.vue"],"sourcesContent":["<template>\n  <div style=\"width: 80%\">\n    <div style=\"margin-bottom: 30px\">新增管理员</div>\n    <el-form :inline=\"true\" :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\n      <el-form-item label=\"用户名\" prop=\"username\">\n        <el-input v-model=\"form.username\" placeholder=\"请输入姓名\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"联系方式\" prop=\"phone\">\n        <el-input v-model=\"form.phone\" placeholder=\"请输入联系方式\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"邮箱\" prop=\"email\">\n        <el-input v-model=\"form.email\" placeholder=\"请输入地址\"></el-input>\n      </el-form-item>\n    </el-form>\n\n    <div style=\"text-align: center; margin-top: 30px\">\n      <el-button type=\"primary\" @click=\"save\" size=\"medium\">提交</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'AddAdmin',\n  data() {\n    const checkPhone = (rule, value, callback) => {\n      if (!/^[1][3,4,5,6,7,8,9][0-9]{9}$/.test(value)) {\n        callback(new Error('请输入合法的手机号'));\n      }\n      callback()\n    };\n    return {\n      form: {},\n      rules: {\n        username: [\n          { required: true, message: '请输入用户名', trigger: 'blur'},\n          { min: 3, max: 10, message: '长度在3-10个字符', trigger: 'blur'}\n        ],\n        phone: [\n          { validator: checkPhone, trigger: 'blur' }\n        ]\n      }\n    }\n  },\n  methods: {\n    save() {\n      this.$refs['ruleForm'].validate((valid) => {\n        if (valid) {\n          request.post('/admin/save', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success('新增成功')\n              this.$refs['ruleForm'].resetFields()\n            } else {\n              this.$notify.error(res.msg)\n            }\n          })\n        }\n      })\n    }\n  }\n}\n\n</script>\n\n"]},"metadata":{},"sourceType":"module"}