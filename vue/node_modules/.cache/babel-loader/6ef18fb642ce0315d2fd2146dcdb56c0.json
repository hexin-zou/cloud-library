{"ast":null,"code":"import request from \"@/utils/request\";\nimport Cookies from 'js-cookie';\nexport default {\n  name: 'BorrowList',\n\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        bookNo: ''\n      }\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get('/borrow/borrow/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      };\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n\n    del(id) {\n      request.delete(\"/borrow/borrow/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n\n    returnBooks(row) {\n      request.post(\"/borrow/borrow/saveRetur\", row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('还书成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoEA;AACA;AAEA;EACAA,kBADA;;EAEAC;IACA;MACAC,mEADA;MAEAC,aAFA;MAGAC,QAHA;MAIAC;QACAC,UADA;QAEAC,YAFA;QAGAP,QAHA;QAIAQ;MAJA;IAJA;EAWA,CAdA;;EAeAC;IACA;EACA,CAjBA;;EAkBAC;IACAC;MACAC;QACAP;MADA,GAEAQ,IAFA,CAEAC;QACA;UACA;UACA;QACA;MACA,CAPA;IAQA,CAVA;;IAWAC;MACA;QACAT,UADA;QAEAC,YAFA;QAGAS,YAHA;QAIAR,UAJA;QAKAS;MALA;MAOA;IACA,CApBA;;IAqBAC;MACA;MACA;MACA;IACA,CAzBA;;IA0BAC;MACAP;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAnCA;;IAoCAQ;MACAR;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA;;EA7CA;AAlBA","names":["name","data","admin","tableData","total","params","pageNum","pageSize","bookNo","created","methods","load","request","then","res","reset","bookName","userName","handleCurrentChange","del","returnBooks"],"sourceRoot":"src/views/borrow","sources":["List.vue"],"sourcesContent":["<template>\n  <div>\n    <!--    搜索表单-->\n    <div style=\"margin-bottom: 20px\">\n      <el-input style=\"width: 240px\" placeholder=\"请输入图书名称\" v-model=\"params.bookName\"></el-input>\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入图书标准码\" v-model=\"params.bookNo\"></el-input>\n      <el-input style=\"width: 240px; margin-left: 5px\" placeholder=\"请输入用户名称\" v-model=\"params.userName\"></el-input>\n      <el-button style=\"margin-left: 5px\" type=\"primary\" @click=\"load\"><i class=\"el-icon-search\"></i> 搜索</el-button>\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\"><i class=\"el-icon-refresh\"></i> 重置</el-button>\n    </div>\n\n    <el-table :data=\"tableData\" stripe row-key=\"id\"  default-expand-all>\n      <el-table-column prop=\"id\" label=\"编号\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"bookName\" label=\"图书名称\"></el-table-column>\n      <el-table-column prop=\"bookNo\" label=\"图书标准码\"></el-table-column>\n      <el-table-column prop=\"userNo\" label=\"会员码\"></el-table-column>\n      <el-table-column prop=\"userName\" label=\"用户名称\"></el-table-column>\n      <el-table-column prop=\"userPhone\" label=\"用户联系方式\"></el-table-column>\n      <el-table-column prop=\"score\" label=\"所用积分\"></el-table-column>\n      <el-table-column prop=\"createtime\" label=\"借出日期\"></el-table-column>\n      <el-table-column prop=\"status\" label=\"借出状态\"></el-table-column>\n      <el-table-column prop=\"days\" label=\"借出天数\"></el-table-column>\n      <el-table-column prop=\"returnDate\" label=\"归还日期\"></el-table-column>\n      <el-table-column prop=\"note\" label=\"过期提醒\">\n        <template v-slot=\"scope\">\n          <el-tag type=\"success\" v-if=\"scope.row.note === '正常'\">{{ scope.row.note }}</el-tag>\n          <el-tag type=\"primary\" v-if=\"scope.row.note === '即将到期'\">{{ scope.row.note }}</el-tag>\n          <el-tag type=\"warning\" v-if=\"scope.row.note === '已到期'\">{{ scope.row.note }}</el-tag>\n          <el-tag type=\"danger\" v-if=\"scope.row.note === '已过期'\">{{ scope.row.note }}</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"管理\">\n        <template v-slot=\"scope\">\n          <el-button type=\"primary\" @click=\"returnBooks(scope.row)\" v-if=\"scope.row.status === '已借出'\">还书</el-button>\n        </template>\n      </el-table-column>\n<!--      <el-table-column prop=\"updatetime\" label=\"更新时间\"></el-table-column>-->\n      <el-table-column label=\"操作\">\n        <template v-slot=\"scope\">\n<!--          scope.row 就是当前行数据-->\n<!--          <el-button type=\"primary\" @click=\"$router.push('/editBorrow?id=' + scope.row.id)\">编辑</el-button>-->\n          <el-popconfirm\n              style=\"margin-left: 5px\"\n              title=\"您确定删除这行数据吗？\"\n              @confirm=\"del(scope.row.id)\"\n          >\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <!--    分页-->\n    <div style=\"margin-top: 20px\">\n      <el-pagination\n          background\n          :current-page=\"params.pageNum\"\n          :page-size=\"params.pageSize\"\n          layout=\"prev, pager, next\"\n          @current-change=\"handleCurrentChange\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\nimport Cookies from 'js-cookie'\n\nexport default {\n  name: 'BorrowList',\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        bookNo: ''\n      }\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load() {\n      request.get('/borrow/borrow/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list\n          this.total = res.data.total\n        }\n      })\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        bookName: '',\n        bookNo: '',\n        userName: ''\n      }\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      // 点击分页按钮触发分页\n      this.params.pageNum = pageNum\n      this.load()\n    },\n    del(id) {\n      request.delete(\"/borrow/borrow/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功')\n          this.load()\n        } else {\n          this.$notify.error(res.msg)\n        }\n      })\n    },\n    returnBooks(row) {\n      request.post(\"/borrow/borrow/saveRetur\", row).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('还书成功')\n          this.load()\n        } else {\n          this.$notify.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}